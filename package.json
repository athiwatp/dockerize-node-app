{
  "name": "dockerize-app",
  "version": "2.0.1",
  "description": "CLI tool to launch a docker container and create a Dockerfile for NodeJS applications",
  "preferGlobal": true,
  "bin": {
    "dockerize": "index.js"
  },
  "scripts": {
    "test": "xo index.js lib/** tests/** && mocha tests",
    "watch": "chokidar index.js lib/** tests/** -c 'npm test'",
    "retire": "retire"
  },
  "xo": {
    "envs": [
      "node",
      "mocha"
    ],
    "space": true,
    "rules": {
      "arrow-body-style": [
        0
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brainsiq/dockerize-node-app.git"
  },
  "keywords": [
    "docker",
    "dockerfile",
    "dockerize",
    "dockerise",
    "containerize",
    "containerise"
  ],
  "author": {
    "name": "Chris Impey",
    "email": "chris@impey.io"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brainsiq/dockerize-node-app/issues"
  },
  "homepage": "https://github.com/brainsiq/dockerize-node-app#readme",
  "devDependencies": {
    "chai": "^3.4.1",
    "chokidar-cli": "^1.2.0",
    "dirty-chai": "^1.2.2",
    "mocha": "^2.3.4",
    "nock": "^3.3.2",
    "retire": "^1.1.1",
    "xo": "^0.11.2"
  },
  "dependencies": {
    "lodash-deep": "^1.6.0",
    "request-promise": "^1.0.2"
  }
}
